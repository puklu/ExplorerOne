{
    "editor.fontSize": 20,
    "terminal.integrated.fontSize": 20,
    "workbench.activityBar.location": "top",
    "cmake.pinnedCommands": [
        "workbench.action.tasks.configureTaskRunner",
        "workbench.action.tasks.runTask"
    ],
    "cmake.showOptionsMovedNotification": false,
    "editor.minimap.enabled": false,
    "window.customTitleBarVisibility": "auto",
    "window.titleBarStyle": "custom",
    "diffEditor.ignoreTrimWhitespace": false,

    "files.associations": {
        "stdint.h": "c",
        "errno.h": "c",
        "cstdint": "cpp",
        "cstdarg": "cpp",
        "*.tcc": "cpp",
        "format": "cpp",
        "cstdio": "cpp",
        "chrono": "cpp",
        "array": "cpp",
        "atomic": "cpp",
        "bit": "cpp",
        "cctype": "cpp",
        "clocale": "cpp",
        "cmath": "cpp",
        "codecvt": "cpp",
        "compare": "cpp",
        "concepts": "cpp",
        "cstddef": "cpp",
        "cstdlib": "cpp",
        "cwchar": "cpp",
        "cwctype": "cpp",
        "deque": "cpp",
        "string": "cpp",
        "unordered_map": "cpp",
        "vector": "cpp",
        "exception": "cpp",
        "algorithm": "cpp",
        "functional": "cpp",
        "iterator": "cpp",
        "memory": "cpp",
        "memory_resource": "cpp",
        "numeric": "cpp",
        "optional": "cpp",
        "random": "cpp",
        "string_view": "cpp",
        "system_error": "cpp",
        "tuple": "cpp",
        "type_traits": "cpp",
        "utility": "cpp",
